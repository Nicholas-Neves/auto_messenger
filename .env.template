# Supabase (Dashboard → Settings → API)
SUPABASE_URL=https://<seu-projeto>.supabase.co
SUPABASE_KEY=<sua_anon_ou_service_key>

# Tabela/colunas (ajuste se seus nomes forem diferentes)
CONTACTS_TABLE=contacts
NAME_COLUMN=name
PHONE_COLUMN=phone

# Z-API (pegue no painel da Z-API)
ZAPI_INSTANCE=<sua_instance_id>
ZAPI_TOKEN=<token_da_instancia>
ZAPI_CLIENT_TOKEN=<seu_security_token>